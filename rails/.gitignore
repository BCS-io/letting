# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal
/db/*.sqlite3-*

# Ignore all logfiles and tempfiles.
/log/*.log
/log/*
!/log/.keep
/tmp

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

public/assets
.byebug_history

#  Ignore master key for decrypting credentials and more.
/config/master.key

# Sandi Meter
/sandi_meter

# SimpleCov
/coverage

# data importing - not saved on github!
/import_data

Guardfile


# Personal TODO
todo/work.TODO

# brakeman
brakeman.html

# Ignore files with secrets
.env
config/secrets.yml
config/app_environment_variables.rb

# capistrano-db-tasks - dump files
/db/*.sql.*

# Ignore secret configuration
/config/secret.yml

# sequence png files
/sequence/*.png

# Mac hidden file - of no relevance
.DS_Store

# ctags - navigation of a codebase
/tags

# backup files
letting.tar
PostgreSQL.sql
*.dump

# rails-erd
erd.pdf

# ignore terraform binaries
.terraform

# ignore state file which include plain text passwords
*.tfstate
*.tfstate.*

# Ignore override files as they are usually used to override resources locally and so
# are not checked in
override.tf
override.tf.json
*_override.tf
*_override.tf.json

# tfvars including env that should not be saved
*.tfvars


# ansible
secrets.yml

*.retry
