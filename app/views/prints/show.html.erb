<div class='grid''>
  <div class='col-10-24'>
  <% @title='Invoicing' %>
  </div>
  <div class='col-10-24 r-align'>
  <%= link_to fa_icon('print 2x'), '#',title: 'Print', class: 'no-print js-printoff' %>
  </div>
</div>

<% @invoicing.runs.last.invoices.each do |invoice| %>

  <%= render 'invoices/invoice_page1', invoice: invoice %>
  <div class='page-break'>
    &nbsp;
  </div>
  <%= render 'invoice_page2/invoice_page2', invoice: invoice %>
<% end %>
