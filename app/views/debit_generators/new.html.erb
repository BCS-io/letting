<% @title='New Invoicing' %>

<%= form_for @debit_generator do |debit_gen_form| %>
  <%= render 'shared/errors', object: @debit_generator %>
  <fieldset id='debit_generator' class='shell' style='position: relative;'>
    <%= render 'search_chargeable_properties', debit_gen_form: debit_gen_form %>
    <%= render 'chargeable_properties', debit_gen_form: debit_gen_form, debit_generator: @debit_generator %>
  </fieldset>
<% end %>
