<% @title='View Invoice Text' %>
<div id='template' class='shell  template-padding'>
  <div class='col-12-24  bottom-spacer'>
    &nbsp;
  </div>
  <div class='col-12-24  bottom-spacer'>
    <%= edit_link @template %>
  </div>

  Template Number <%= @template.id %>

  <% if @template.id == 1 %>
    <%= render 'front_page', template_front: @template %>
  <% else %>
    <%= render 'page2', template_back: @template %>
  <% end %>
</div>
