<% if object.use_profile? %>
  <% if object.entities.any? %>
     <% object.entities.each do |entity| %>
    <%= render 'shared/show_entity', object: entity %>
  <% end %>
  <br>
<%= render 'shared/show_billing_address', object: object.bill_to %>
<% else %>
  No Entities
<% end %>
<% else %>
    <% @property.entities.each do |entity| %>
        <%= render 'shared/show_entity', object: entity %>
      <% end %>
 <br>
 <%= render 'shared/show_billing_address', object:@property %>

    <% end %>