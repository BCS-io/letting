<!DOCTYPE html>
<html>
  <%= render 'layouts/head', title: @title %>
  <body>
    <%= render 'layouts/header' %>
    <div class='view-wrapper'>
      <article>
        <h2 class = 'no-print'><%= @title %></h2>
        <%= yield %>
      </article>
      <aside class = 'side-wrap  float-right'>
        <%= render 'layouts/sidebar' %>
      </aside>
    </div>
  </body>
</html>
