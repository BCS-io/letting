<article>
  <header>
    <h3 class = "mainTitle">Property</h3>
  </header>
  <p>
    <div class='fieldset' style='background: #ddd;border:1px solid #666; border-radius: 12px; padding: 0 20px;margin-top: 10px;margin-bottom: 3px;'>
    Property Id <%= @property.human_id %> <%= link_to 'Edit', edit_property_path(@property) %> |
  </p>
  <h3>Address</h3>
  <%= render 'shared/show_address', object: @property %>
  <h3>Tenant</h3>
  <% @property.entities.each do |entity| %>
    <%= render 'shared/show_entity', object: entity %>
  <% end %>

  <%= render 'show_billing_profile', object: @property.billing_profile %>
  <%= render 'show_charges', object: @property.charges %>
  </div>
</article>
