<% invoicing_dec = InvoicingIndexDecorator.new invoicing %>
<div title='View invoice run' class='table-row' data-link='<%= run_path(run) %>'>

  <div class='col-5-24  grid-row  inherit-row'>
    <%= invoicing_dec.property_range %>
  </div>
  <div class='col-5-24  inherit-row  bottom-line-spacer'>
    <%= invoicing_dec.created_at %>
  </div>
  <div class='col-7-24  inherit-row  bottom-line-spacer'>
    <%= invoicing_dec.period_between %>
  </div>
  <div class='col-1-24  bottom-line-spacer'>
    <%= print_link path: print_run_path(invoicing.runs.last)  %>
  </div>
  <div class='col-2-24  bottom-line-spacer'>&nbsp;</div>
  <div class='col-1-24  bottom-line-spacer'>
    <%= delete_link path: invoicing %>
  </div>
  <div class='col-3-24  bottom-line-spacer'>&nbsp;</div>
</div>