<% @title='View Sheet' %>

<div class='col-1-2 vertical-spacer'>
  &nbsp;
</div>
<div class='col-1-2 vertical-spacer'>
  <%= link_to fa_icon('edit'), edit_sheet_path(@sheet), class: 'simple-button float-right', title: 'Edit' %>
</div>

Sheet Number <%= @sheet.id %>

<% if @sheet.id == 1 %>
  <%= render '/sheets/page1', sheet: @sheet %>
<% else %>
  <%= render '/sheets/page2', sheet: @sheet %>
<% end %>
